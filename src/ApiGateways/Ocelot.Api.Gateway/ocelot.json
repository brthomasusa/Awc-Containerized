{
    "Routes": [
        {
            "DownstreamPathTemplate": "/api/v1/Company",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "localhost",
                    "Port": 5052
                }
            ],
            "UpstreamPathTemplate": "/Company",
            "UpstreamHttpMethod": [
                "GET"
            ],
            "RateLimitOptions": {
                "ClientWhitelist": [],
                "EnableRateLimiting": true,
                "Period": "5s",
                "PeriodTimespan": 1,
                "Limit": 1
            }
        }
    ],
    "FileCacheOptions": {
        "TtlSeconds": 20,
        "Region": "SomeRegionName"
    },
    "GlobalConfiguration": {
        "BaseUrl": "http://localhost:5093",
        "RateLimitOptions": {
            "DisableRateLimitHeaders": false,
            "QuotaExceededMessage": "Too many requests!!!",
            "HttpStatusCode": 429,
            "ClientIdHeader": "ClientId"
        }
    }
}